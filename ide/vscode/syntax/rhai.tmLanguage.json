          "name": "meta.function.closure.rhai",
          "begin": "(\\b[_a-zA-Z]\\w*)\\s*(\\:)\\s*(\\|)",
          "beginCaptures": {
            "1": {
              "name": "string.unquoted.label.rhai entity.name.function.method.rhai"
