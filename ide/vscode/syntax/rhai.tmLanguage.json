    "comments": {
      "patterns": [
        {
          "name": "comment.block.documentation.rhai",
          "begin": "/\\*\\*(?![/|\\*])",
          "end": "\\*/",
          "captures": {
            "0": {
              "name": "punctuation.definition.comment.block.documentation.rhai"
            }
          },
          "patterns": [
            {
              "include": "#comments"
