    "literal-function-call": {
      "patterns": [
        {
          "name": "meta.function-call.without-arguments.rhai",
          "match": "(?x)  ([_a-zA-Z]\\w*)(!)?\\s*\n  (\\(\\s*\\))",
          "captures": {
